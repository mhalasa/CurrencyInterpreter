function fun(a, b){
	
	i = 0;				
	if (a > b){
		while(i < 10){
			a = a + 2*b - i;
			i = i + 1;
		}
		return a;		
	}else{
		i = 14;
		c = (i*b - i /5 dol - a) pln;
		return c dol;			
	}
}


function main()
{
	x =3 dol;
